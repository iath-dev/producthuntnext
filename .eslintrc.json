{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb",
        "plugin:prettier/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "React": "writable"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "indent": 0,
        "no-tabs": 0,
        "eol-last": ["error", "always"],
        "no-underscore-dangle": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/forbid-prop-types": 0,
        "react/require-default-props": 0
    }
}